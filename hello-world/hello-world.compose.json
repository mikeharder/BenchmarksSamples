{
    "$schema": "https://raw.githubusercontent.com/aspnet/Benchmarks/master/src/BenchmarksDriver2/benchmarks.schema.json",
    "scenarios": {
      "helloworld": {
        "application": {
          "job": "helloworld"
        }
      }
    },
    "jobs": {
      "helloworld": {
        "source": {
          "repository": "https://github.com/mikeharder/BenchmarksSamples.git",
          "branchOrCommit": "master",
          "dockerfile": "hello-world/dockerfile"
        },
        "waitForExit": true,
        "readyStateText": "Application started."
      }
    }
  }